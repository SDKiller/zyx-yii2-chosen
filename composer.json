{
    "name": "zyx/yii2-chosen",
    "type": "yii2-extension",
    "description": "Bootstrap Chosen widget for Yii2, forked from nex/yii2-chosen",
    "keywords": ["yii2", "extension", "widget", "chosen", "bootstrap", "bootstrap 3"],
    "homepage": "https://github.com/RomeroMsk/yii2-chosen",
    "license": "BSD 3-Clause",
    "authors": [
        {
            "name": "Roman Ovchinnikov",
            "email": "nex.software@gmail.com",
            "homepage": "https://github.com/RomeroMsk"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/chosen": "*"
    },
    "extra": {
        "asset-repositories": [
            {
                "name": "bower-asset/chosen",
                "type": "bower-vcs",
                "url": "https://github.com/harvesthq/chosen/releases/download/v1.2.0/chosen_v1.2.0.zip"
            }
        ]
    },
    "autoload": {
        "psr-4": {
            "zyx\\chosen\\": ""
        }
    }
}
